{
  "name": "cypress-automation",
  "version": "1.0.0",
  "description": "Cypress Automation - TS -JS",
  "main": "index.js",
  "scripts": {
    "electron": "cypress run --spec  cypress/e2e/25.pageObjects.cy.ts",
    "allTest": "cypress run --e2e --browser chrome",
    "runner": "cypress open --e2e --browser chrome --config baseUrl=http://uitestingplayground.com --env demoVar=Hello_From_Package_File",
    "production_Test": "cypress open --e2e --browser chrome --config baseUrl=http://www.google.com --env demoVar=Hello_From_Package_File",
    "cypress-run": "cypress run --browser firefox --spec cypress/e2e/40.bookAPI.cy.ts --record --key 8fa5e574-d0c0-45b8-b917-b5c7ed24e7f9",
    "cypress-parallel": "cypress run --record --key 8fa5e574-d0c0-45b8-b917-b5c7ed24e7f9 --parallel --spec cypress/e2e/parallel-test/*.ts --browser chrome"
  },
  "keywords": [
    "cypress",
    "javascript",
    "typescript",
    "testing"
  ],
  "author": "Meris",
  "license": "ISC",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.3",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/cypress": "^9.0.0",
    "cy-verify-downloads": "^0.1.13",
    "cypress": "^12.9.0",
    "cypress-dotenv": "^2.0.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.3.0",
    "cypress-real-events": "^1.7.6",
    "dotenv": "^16.0.3",
    "node-xlsx": "^0.21.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress-plugin-xhr-toggle": "^1.0.0",
    "mysql": "github:mysqljs/mysql"
  }
}
